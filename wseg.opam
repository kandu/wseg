opam-version: "2.0"
maintainer: "zandoye@gmail.com"
authors: [ "ZAN DoYe" ]
homepage: "https://github.com/kandu/wseg/"
bug-reports: "https://github.com/kandu/wseg/issues"
license: "MIT"
dev-repo: "git://github.com/kandu/wseg.git"
build: [
  ["dune" "build" "-p" name "-j" jobs]
